{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["app/components/*"],
      "@/screens/*": ["app/screens/*"],
      "@/types/*": ["app/types/*"],
      "@/lib/*": ["app/lib/*"],
      "@/assets/*": ["app/assets/*"],
      "@/hooks/*": ["app/hooks/*"],
      "@/navigation/*": ["app/navigation/*"],
      "@/store/*": ["app/store/*"],
      "@/api/*": ["app/api/*"],
      "@/utils/*": ["app/utils/*"]
    }
  }
}
